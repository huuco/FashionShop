<tr class="odd gradeX" align="center">
  <td><%= user.id %></td>
  <td><%= user.full_name %></td>
  <td><%= user.username %></td>
  <td><%= user.email %></td>
  <td><%= user.role %></td>
  <td><%= user.active %></td>
  <td class="center">
    <i class="fa fa-pencil fa-fw"></i>
    <%= link_to t("admin.edit"), edit_admin_user_path(user) %>
  </td>
  <td class="center">
      <i class="fa fa-trash-o fa-fw"></i>
      <%= link_to t(".delete"), admin_user_path(user),
        method: :delete, data: {confirm: t("admin.you_sure")} %>
  </td>
</tr>
